<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Listening.Fragment_listen_question2">

    <!-- TODO: Update blank fragment layout -->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/q5"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="30dp"
                android:text="5 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/ans5"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q5"
                app:layout_constraintStart_toEndOf="@+id/q5"
                app:layout_constraintTop_toTopOf="@+id/q5"
                app:layout_constraintVertical_bias="0.607" />

            <TextView
                android:id="@+id/q4"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="40dp"
                android:text="4 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/q5"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/ans4"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q4"
                app:layout_constraintStart_toEndOf="@+id/q4"
                app:layout_constraintTop_toTopOf="@+id/q4"
                app:layout_constraintVertical_bias="0.636" />

            <TextView
                android:id="@+id/textView38"
                android:layout_width="113dp"
                android:layout_height="32dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="section 1"
                android:textSize="22sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.example.gproject.JustifyTextView2
                android:id="@+id/Lquestion"
                android:layout_width="369dp"
                android:layout_height="106dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="18dp"
                android:background="@color/gray"
                android:text="Question"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView38" />

            <ImageView
                android:id="@+id/pic"
                android:layout_width="410dp"
                android:layout_height="250dp"
                android:layout_marginTop="12dp"
                app:layout_constraintBottom_toTopOf="@+id/q"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Lquestion">

            </ImageView>

            <TextView
                android:id="@+id/q3"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="40dp"
                android:text="3 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/q4"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/ans3"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q3"
                app:layout_constraintStart_toEndOf="@+id/q3"
                app:layout_constraintTop_toTopOf="@+id/q3"
                app:layout_constraintVertical_bias="0.727" />

            <TextView
                android:id="@+id/q2"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="40dp"
                android:text="2 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/q3"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/ans2"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q2"
                app:layout_constraintStart_toEndOf="@+id/q2"
                app:layout_constraintTop_toTopOf="@+id/q2"
                app:layout_constraintVertical_bias="0.454" />

            <TextView
                android:id="@+id/q"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="40dp"
                android:text="1 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/q2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/pic" />

            <EditText
                android:id="@+id/ans"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q"
                app:layout_constraintStart_toEndOf="@+id/q"
                app:layout_constraintTop_toTopOf="@+id/q"
                app:layout_constraintVertical_bias="0.409" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</FrameLayout>