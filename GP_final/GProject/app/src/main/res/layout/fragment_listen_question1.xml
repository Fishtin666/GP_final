<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Listening.Fragment_listen_question1">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/q5"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="30dp"
                android:text="5 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/ans5"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q5"
                app:layout_constraintStart_toEndOf="@+id/q5"
                app:layout_constraintTop_toTopOf="@+id/q5"
                app:layout_constraintVertical_bias="0.607" />

            <TextView
                android:id="@+id/q4"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="40dp"
                android:text="4 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/q5"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/ans4"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q4"
                app:layout_constraintStart_toEndOf="@+id/q4"
                app:layout_constraintTop_toTopOf="@+id/q4"
                app:layout_constraintVertical_bias="0.636" />

            <TextView
                android:id="@+id/textView38"
                android:layout_width="113dp"
                android:layout_height="32dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="20dp"
                android:text="section 1"
                android:textSize="22sp"
                app:layout_constraintBottom_toTopOf="@+id/Lquestion"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.example.gproject.JustifyTextView2
                android:id="@+id/Lquestion"
                android:layout_width="374dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="64dp"
                android:background="@color/gray"
                android:text="Question"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/q"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/q3"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="40dp"
                android:text="3 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/q4"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/ans3"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q3"
                app:layout_constraintStart_toEndOf="@+id/q3"
                app:layout_constraintTop_toTopOf="@+id/q3"
                app:layout_constraintVertical_bias="0.727" />

            <TextView
                android:id="@+id/q2"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="40dp"
                android:text="2 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/q3"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/ans2"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q2"
                app:layout_constraintStart_toEndOf="@+id/q2"
                app:layout_constraintTop_toTopOf="@+id/q2"
                app:layout_constraintVertical_bias="0.454" />

            <TextView
                android:id="@+id/q"
                android:layout_width="30dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginBottom="40dp"
                android:text="1 ."
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/q2"
                app:layout_constraintStart_toStartOf="parent" />

            <EditText
                android:id="@+id/ans"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:ems="10"
                android:inputType="text"
                app:layout_constraintBottom_toBottomOf="@+id/q"
                app:layout_constraintStart_toEndOf="@+id/q"
                app:layout_constraintTop_toTopOf="@+id/q"
                app:layout_constraintVertical_bias="0.409" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</FrameLayout>